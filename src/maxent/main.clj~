(in-ns 'maxent.core)

(def test-sample (take 15 (lazy-file-lines "/home/matt/Documents/clojure/MaxEnt/resources/train.txt")))

(def feature-list (transform-data 3 [:text :pos :chunk] test-sample))

(def y (select-values ))

(def features  (remove #(= % :pos0) (keys (first (assemble-features feature-list)))))


(def f (assemble-features feature-list))

(println (choices? :pos0 f))

;;(println (count-features history-keys f))

;;(println f)

;; weights are a map of lists, one key for each outcome.
(def weights {})

(def model (train-sgd weights .001 20 f))
